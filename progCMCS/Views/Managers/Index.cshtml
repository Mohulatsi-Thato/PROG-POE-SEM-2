@model IEnumerable<progCMCS.Models.Manager>

<h2>Managers</h2>
<p><a href="/Managers/Create">+ New Manager</a></p>

<table border="1" cellpadding="5">
    <tr><th>ID</th><th>Name</th><th>Role</th><th>Email</th></tr>
    @foreach (var m in Model)
    {
        <tr>
            <td>@m.ManagerID</td>
            <td>@m.Name</td>
            <td>@m.Role</td>
            <td>@m.Email</td>
        </tr>
    }
</table>
